@model IEnumerable<WebStore9.Models.Employee>

<div class="container">
    <h1>Сотрудники</h1>
    <hr/>

    <table>
        <thead>
            <tr>
                <th>#</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
                <th>Seniority</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.LastName</td>
                    <td>@employee.FirstName</td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                    <td>@employee.Seniority</td>
                </tr>
            }
        </tbody>
    </table>
    
</div>
